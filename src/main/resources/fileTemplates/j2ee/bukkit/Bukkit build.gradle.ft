plugins {
    id 'java'
}

group = '${GROUP_ID}'
version = '${PLUGIN_VERSION}'

java {
    sourceCompatibility = JavaVersion.VERSION_1_8
    targetCompatibility = JavaVersion.VERSION_1_8
}

repositories {
    mavenCentral()
}

dependencies {
}

processResources {
    def props = [version: version]
    inputs.properties props
    expand props
}
